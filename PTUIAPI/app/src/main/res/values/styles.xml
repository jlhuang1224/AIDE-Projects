<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppTheme" parent="@android:style/Theme.Holo.Light.NoActionBar">
	</style>
	<style name="textview_title_dark_style" >
		<item name="android:textColor">#333333</item>
		<item name="android:textSize">26sp</item>
	</style>
	<style name="textview_text_dark_style" >
		<item name="android:textColor">#555555</item>
		<item name="android:textSize">18sp</item>
	</style>
	<style name="textview_title_light_style" >
		<item name="android:textColor">#f0f0f0</item>
		<item name="android:textSize">26sp</item>
	</style>
	<style name="textview_text_light_style" >
		<item name="android:textColor">#e0e0e0</item>
		<item name="android:textSize">18sp</item>
	</style>
	<style name="button_left_style" >
		<item name="android:textColor">#555555</item>
		<item name="android:layout_height">50dp</item>
		<item name="android:background">@drawable/button_left_shape_selector</item>
	</style>
	<style name="button_center_style" >
		<item name="android:textColor">#555555</item>
		<item name="android:layout_height">50dp</item>
		<item name="android:background">@drawable/button_center_shape_selector</item>
	</style>
	<style name="button_right_style" >
		<item name="android:textColor">#555555</item>
		<item name="android:layout_height">50dp</item>
		<item name="android:background">@drawable/button_right_shape_selector</item>
	</style>
	<style name="button_center_one_style" >
		<item name="android:textColor">#555555</item>
		<item name="android:layout_height">50dp</item>
		<item name="android:background">@drawable/button_center_one_shape_selector</item>
	</style>
	<style name="button_round_style" >
		<item name="android:textColor">#555555</item>
		<item name="android:layout_height">50dp</item>
		<item name="android:layout_width">50dp</item>
		<item name="android:background">@drawable/button_round_shape_selector</item>
	</style>
	<style name="button_left_translucent_style" >
		<item name="android:textColor">#eef0f0f0</item>
		<item name="android:layout_height">50dp</item>
		<item name="android:background">@drawable/button_left_shape_selector_translucent</item>
	</style>
	<style name="button_center_translucent_style" >
		<item name="android:textColor">#eef0f0f0</item>
		<item name="android:layout_height">50dp</item>
		<item name="android:background">@drawable/button_center_shape_selector_translucent</item>
	</style>
	<style name="button_right_translucent_style" >
		<item name="android:textColor">#eef0f0f0</item>
		<item name="android:layout_height">50dp</item>
		<item name="android:background">@drawable/button_right_shape_selector_translucent</item>
	</style>
	<style name="button_center_one_translucent_style" >
		<item name="android:textColor">#eef0f0f0</item>
		<item name="android:layout_height">50dp</item>
		<item name="android:background">@drawable/button_center_one_shape_selector_translucent</item>
	</style>
	<style name="button_round_translucent_style" >
		<item name="android:textColor">#eef0f0f0</item>
		<item name="android:layout_height">50dp</item>
		<item name="android:layout_width">50dp</item>
		<item name="android:background">@drawable/button_round_shape_selector_translucent</item>
	</style>
	<style name="frame_button_textview_style">
		<item name="android:textColor">#333333</item>
		<item name="android:textSize">14dp</item>
		<item name="android:layout_gravity">center</item>
		<item name="android:singleLine">true</item>
	</style>
	<style name="edittext_style" >
		<item name="android:textColor">#333333</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:padding">5dp</item>
		<item name="android:background">@drawable/edittext_shape_selector</item>
	</style>
	<style name="edittext_translucent_style" >
		<item name="android:textColor">#eef0f0f0</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:padding">5dp</item>
		<item name="android:background">@drawable/edittext_shape_selector_translucent</item>
	</style>
	<style name="checkbox_style">
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_width">wrap_content</item>
		<item name="android:textColor">#555555</item>
		<item name="android:textSize">18sp</item>
		<item name="android:button">@drawable/checkbox_selector</item>
	</style>
	<style name="checkbox_translucent_style">
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_width">wrap_content</item>
		<item name="android:textColor">#e0e0e0</item>
		<item name="android:textSize">18sp</item>
		<item name="android:background">#60000000</item>
		<item name="android:button">@drawable/checkbox_selector</item>
	</style>
	<style name="radiobutton_style">
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_width">wrap_content</item>
		<item name="android:textColor">#555555</item>
		<item name="android:textSize">18sp</item>
		<item name="android:button">@drawable/radiobutton_selector</item>
	</style>
	<style name="radiobutton_translucent_style">
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_width">wrap_content</item>
		<item name="android:textColor">#e0e0e0</item>
		<item name="android:textSize">18sp</item>
		<item name="android:background">#60000000</item>
		<item name="android:button">@drawable/radiobutton_selector</item>
	</style>
	<style name="progressbar_ring_style" parent="@android:style/Widget.ProgressBar">
		<item name="android:indeterminateDrawable">@drawable/progressbar_ring</item>
		<item name="android:indeterminateDuration">800</item>
	</style>
	<style name="progressbar_indeterminate_horizontal_style" parent="@android:style/Widget.ProgressBar.Horizontal">
		<item name="android:indeterminate">true</item>
		<item name="android:indeterminateDrawable">@drawable/progressbar_indeterminate_horizontal</item>
		<item name="android:layout_height">8dp</item>
	</style>
	<style name="progressbar_horizontal_style" parent="@android:style/Widget.ProgressBar.Horizontal">
		<item name="android:progressDrawable">@drawable/progressbar_shape_horizontal</item>
		<item name="android:layout_height">6dp</item>
	</style>
	<style name="seekbar_style" parent="@android:style/Widget.ProgressBar.Horizontal">
		<item name="android:thumb">@drawable/seekbar_thumb_selector</item>
		<item name="android:progressDrawable">@drawable/seekbar_progress_shape</item>
		<item name="android:maxHeight">6dp</item>
	</style>
	<style name="ptui_title_button_back_style">
		<item name="android:layout_height">70dp</item>
		<item name="android:layout_width">wrap_content</item>
		<item name="android:background">#00000000</item>
		<item name="android:padding">10dp</item>
		<item name="android:textColor">@drawable/ptui_title_back_selector</item>
		
	</style>
</resources>
